{
  "name": "nsw-design-system-plone6",
  "version": "0.8.6",
  "description": "nsw-design-system-plone6: Volto add-on",
  "main": "src/index.js",
  "license": "MIT",
  "type": "commonjs",
  "keywords": [
    "volto-addon",
    "volto",
    "plone",
    "react"
  ],
  "scripts": {
