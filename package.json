    "release": "npx release-it",
    "test": "jest --env=@happy-dom/jest-environment"
  },
  "addons": [
    "@eeacms/volto-accordion-block",
    "@kitconcept/volto-blocks-grid",
    "volto-form-block",
    "volto-google-analytics",
    "volto-subfooter",
    "volto-siteinfo",
    "@eeacms/volto-columns-block",
    "@plone-collective/volto-slots-editor"
  ],
  "dependencies": {
    "@eeacms/volto-accordion-block": "10.4.6",
    "@eeacms/volto-columns-block": "6.3.2",
    "@fontsource/public-sans": "4.5.12",
    "@kitconcept/volto-blocks-grid": "5.2.0",
    "@material-design-icons/svg": "0.14.6",
    "@mdi/svg": "7.2.96",
    "classnames": "2.5.1",
    "html-react-parser": "3.0.16",
    "material-icons": "1.13.6",
    "moment": "2.24.0",
    "nsw-design-system": "3.16.0",
    "react-color": "2.19.3",
    "volto-google-analytics": "2.0.0",
    "volto-siteinfo": "1.1.0",
    "volto-subblocks": "2.0.0",
    "volto-subfooter": "2.0.0"
  },
  "peerDependencies": {
    "@plone-collective/volto-slots-editor": "*",
    "@plone/volto": ">= 16.20.1 < 17.0.0",
    "react": ">= 16.8.0",
    "react-dom": ">= 16.8.0",
    "volto-form-block": ">=3.0.0"
  },
  "devDependencies": {
    "@babel/types": "7.20.5",
    "@happy-dom/jest-environment": "14.12.3",
    "@plone/scripts": ">= 2.2.2",
    "@plone/volto": "16.20.1",
    "@testing-library/react": "14.3.1",
    "babel-plugin-react-intl": "5.1.17",
    "babel-preset-razzle": "4.2.18",
    "jest": "29.7.0",
    "mrs-developer": "2.1.0",
    "razzle": "4.2.18",
    "razzle-dev-utils": "4.2.18",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "release-it": "^15.4.2"
  },
  "jest": {
    "transformIgnorePatterns": [
      "/node_modules/(?!@plone/volto).+\\.js$"
    ],
    "moduleNameMapper": {
      "@plone/volto/(.*)$": "<rootDir>/node_modules/@plone/volto/src/$1"
    }
  }
}
